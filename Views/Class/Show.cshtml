@model SchoolProject.Models.Class
@{
    ViewBag.Title = "Show";
}

<a class="navlink" href="/Class/List">Back To Classes</a>
<div id="containershow">
    <h2>@Model.className </h2>

    <p> Class Code: @Model.classCode</p>
    <p> Start Date: @Model.startDate</p>
    <p> Finish Date: @Model.finishDate</p>
    <p> Teacher: <a href="/Teacher/Show/@Model.teacher.teacherId"> @Model.teacher.teacherFname @Model.teacher.teacherLname</a> </p>
</div>
